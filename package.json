{
  "name": "@oruga-ui/theme-oruga",
  "version": "0.5.0",
  "description": "Default theme for Oruga",
  "license": "MIT",
  "type": "module",
  "main": "dist/theme.js",
  "module": "dist/theme.js",
  "unpkg": "dist/theme.js",
  "sideEffects": [
    "*.css",
    "*.scss"
  ],
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oruga-ui/theme-oruga.git"
  },
  "bugs": {
    "url": "https://github.com/oruga-ui/theme-oruga/issues"
  },
  "scripts": {
    "dev": "vite --force",
    "serve": "vite preview",
    "build": "vue-tsc --noEmit && vite build",
    "lint": "eslint . --fix --quiet",
    "lint:style": "stylelint **/*.{css,scss} --fix --ignore-path .gitignore",
    "update": "ncu -u"
  },
  "peerDependencies": {
    "@oruga-ui/oruga-next": "^0.9.0"
  },
  "devDependencies": {
    "@oruga-ui/examples": "0.9.3",
    "@oruga-ui/oruga-next": "0.9.3",
    "@vitejs/plugin-vue": "5.2.1",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.3.0",
    "@vue/tsconfig": "^0.7.0",
    "core-js": "3.40.0",
    "eslint": "^9.18.0",
    "eslint-plugin-prettier": "^5.2.2",
    "eslint-plugin-vue": "^9.32.0",
    "npm-check-updates": "^17.1.14",
    "prettier": "^3.4.2",
    "sass": "1.83.4",
    "stylelint": "^16.13.2",
    "stylelint-config-recommended": "^15.0.0",
    "stylelint-config-recommended-scss": "^14.1.0",
    "stylelint-prettier": "^5.0.2",
    "stylelint-scss": "^6.10.1",
    "typescript": "5.7.3",
    "vite": "^6.0.7",
    "vite-plugin-banner": "^0.8.0",
    "vite-plugin-static-copy": "^2.2.0",
    "vue": "^3.5.13",
    "vue-router": "4.5.0",
    "vue-tsc": "^2.2.0"
  }
}
